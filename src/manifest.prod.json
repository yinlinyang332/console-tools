{
    "name": "控制台版本控制器",
    "version": "1.0",
    "description": "管理控制台版本",
    "manifest_version": 2,
    "minimum_chrome_version": "80",
    "permissions": [],
    "background": {
        "scripts": ["js/background.js"],
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": ["https://github.com/*"],
            "css": ["css/all.css"],
            "js": ["js/all.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/extension-icon-disabled.png"
        }
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "icons": {
        "16": "icons/extension-icon-disabled.png"
    }
}
